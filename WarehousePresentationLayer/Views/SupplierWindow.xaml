<Window x:Class="Warehouse.SupplierWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add Supplier" Height="200" Width="300">
    <Grid Margin="20">
        <StackPanel>
            <TextBlock Text="First Name:"/>
            <TextBox x:Name="txtFirstName" Margin="0,0,0,10"/>

            <TextBlock Text="Last Name:"/>
            <TextBox x:Name="txtLastName" Margin="0,0,0,10"/>

     

            <Button Content="Add Supplier" Width="100" Click="AddSupplier_Click"/>
        </StackPanel>
    </Grid>
</Window>
